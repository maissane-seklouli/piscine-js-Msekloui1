if (name === "David") {

  console.log("I really wanna stay at your house, and i hope this works out");

} else {

  console.log("Get away !");

}

 

============Exo09

 

function choomChoom(a){

if (a%2==0) {

  return 'Choom';

} else {

  return 'ChoomChoom'

}

}

 

============Exo10

function cyberNumber (a){

  if (a%3==0 && a%5==0){

    return 'CyberPunk2077';

  } if (a%3==0){

    return "Cyber";

  } if (a%5==0){

    return "Punk";

  }

   else {

   return 0;

  }

}

============Exo11

 

function bissextile (year){

  if(year%4==0 && year%100!=0 || year%400==0 ){

    console.log("toto");

    return 0;

  } else {

    console.log("bibi");

    return 1;

  }

}

 

============Exo12

function toto(a,b){

  console.log(((a+b)*b)/2);

  return ((a+b)*b)/2

}

 

module.exports = {

  toto

}

 
