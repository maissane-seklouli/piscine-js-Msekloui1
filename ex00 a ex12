function substract(nb1,nb2){

  return nb1-nb2

}

 

============Exo01

 

function add(nb1,nb2){

    return nb1+nb2

}

============Exo02

 

function multiply (nb1,nb2) {

return nb1*nb2

}

============Exo03

 

function divide (a,b){

  if (b===0){

    return("Forbidden")

} else {

    return a/b;

  }  

} 

 

============Exo04

function modulo (nb1,nb2){

  return nb1%nb2

  }

 

